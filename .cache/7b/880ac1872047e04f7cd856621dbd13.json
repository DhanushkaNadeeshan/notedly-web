name":"Header","generated":{"line":79,"column":15},"source":"components/Header.js","original":{"line":75,"column":15}}],"sources":{"components/Header.js":"import React from 'react';\r\nimport { useQuery, gql } from '@apollo/client';\r\nimport { Link , withRouter  } from 'react-router-dom';\r\nimport logo from '../img/logo.svg';\r\nimport styled from 'styled-components';\r\n\r\nimport ButtonAsLink from './ButtonAsLink';\r\n\r\n\r\nconst IS_LOGGED_IN = gql`\r\n  {\r\n    isLoggedIn @client\r\n  }\r\n`;\r\n\r\nconst UserState = styled.div`\r\n  margin-left: auto;\r\n`;\r\n\r\nconst HeaderBar = styled.header`\r\n  width: 100%;\r\n  padding: 0.5em 1em;\r\n  display: flex;\r\n  height: 64px;\r\n  position: fixed;\r\n  align-items: center;\r\n  background-color: #fff;\r\n  color:#000;\r\n  box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.25);\r\n  z-index: 1;\r\n`;\r\n\r\nconst LogoText = styled.h1`\r\n  margin: 0;\r\n  padding: 0;\r\n  display: inline;\r\n`;\r\n\r\nconst Header = props => {\r\n\r\n  const {data , client} = useQuery(IS_LOGGED_IN);\r\n  return (\r\n    <HeaderBar>\r\n      <img src={logo} alt=\"notedly\" height=\"40\" />\r\n      <LogoText>Notedly ✔️</LogoText>\r\n\r\n      <UserState>\r\n        {data.isLoggedIn ? (\r\n          <ButtonAsLink\r\n\r\n          onClick = { () => {\r\n\r\n            // remove token\r\n            localStorage.removeItem('token');\r\n            // clear application cache\r\n            client.resetStore();\r\n            // update local state\r\n            client.cache.writeQuery({ data: { isLoggedIn: false } });\r\n            // redirect the user to the home page\r\n            props.history.push('/');\r\n          }}\r\n          \r\n          >Log Out</ButtonAsLink>\r\n        ) : (\r\n            <p>\r\n              <Link to={'/signin'}>Sign In</Link> or{' '}\r\n              <Link to={'/signup'}>Sign Up</Link>\r\n            </p>\r\n          )}\r\n      </UserState>\r\n    </HeaderBar>\r\n  );\r\n}\r\n\r\nexport default Header;"},"lineCount":null}},"error":null,"hash":"e5209320473b680152561fc7e1d21dff","cacheData":{"env":{}}}