                            id=\"username\"\r\n                            name=\"username\"\r\n                            placeholder=\"username\"\r\n                            onChange={onChange}\r\n                        />\r\n                    </React.Fragment>\r\n                )}\r\n                <label htmlFor=\"email\">Email:</label>\r\n                <input\r\n                    required\r\n                    type=\"email\"\r\n                    id=\"email\"\r\n                    name=\"email\"\r\n                    placeholder=\"Email\"\r\n                    onChange={onChange}\r\n                />\r\n                <label htmlFor=\"password\">Password:</label>\r\n                <input\r\n                    required\r\n                    type=\"password\"\r\n                    id=\"password\"\r\n                    name=\"password\"\r\n                    placeholder=\"Password\"\r\n                    onChange={onChange}\r\n                />\r\n                <Button type=\"submit\">Submit</Button>\r\n            </Form>\r\n        </Wrapper>\r\n    );\r\n}\r\n\r\nexport default UserForm;"},"lineCount":null}},"error":null,"hash":"2a121a047becbbb5d581f99e2cb3d131","cacheData":{"env":{}}}