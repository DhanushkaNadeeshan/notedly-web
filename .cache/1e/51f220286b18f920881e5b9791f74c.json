arge screens\r\nconst UserActions = styled.div`\r\nmargin-left: auto;\r\n`;\r\n\r\nconst Image = styled.div`\r\nmargin :10px;\r\nfloat: right;\r\n`;\r\n\r\nconst Note = ({ note }) => {\r\n\r\n\r\n    const { loading, error, data } = useQuery(IS_LOGGED_IN);\r\n    // if the data is loading, display a loading message\r\n    if (loading) return <p>Loading...</p>;\r\n\r\n    // if there is an error fetching the data, display an error message\r\n    if (error) return <p>Error!</p>;\r\n\r\n    return (\r\n        <StyledNote>\r\n            <MetaData>\r\n                <MetaInfo>\r\n                    <Image>⏱️ {note.createdAt}</Image>\r\n                    <img src={note.author.avatar} alt={`${note.author.username} author`} height=\"50px\" style={{ borderRadius: 25 }} />\r\n                    <em> {note.author.username}</em>  ✍️\r\n            </MetaInfo>\r\n\r\n                {data.isLoggedIn ? (\r\n                    <UserActions>\r\n                        <NoteUser note={note} />\r\n                    </UserActions>\r\n                ) : (\r\n                        <UserActions>\r\n                            😍  {note.favoriteCount}\r\n                        </UserActions>\r\n                    )}\r\n            </MetaData>     \r\n            <ReactMarkdown source={note.content} />\r\n\r\n        </StyledNote>\r\n    );\r\n}\r\n\r\nexport default Note;"},"lineCount":null}},"error":null,"hash":"6929af4ed0b613e26de8528511100d61","cacheData":{"env":{}}}