er favorites list\r\n        props.me.favorites.filter(note => note.id === props.noteId).length > 0\r\n    );\r\n\r\n    const [toggleFavorite] = useMutation(TOGGLE_FAVORITE, {\r\n        variables: {\r\n            id: props.noteId\r\n        },\r\n        // refetch the GET_MY_FAVORITES query to update the cache\r\n        refetchQueries: [{ query: GET_MY_FAVORITES }]\r\n    });\r\n\r\n    return (\r\n        <React.Fragment>\r\n            {favorited ? (\r\n                <ButtonAsLink\r\n                    onClick={() => {\r\n                        toggleFavorite();\r\n                        setFavorited(false);\r\n                        setCount(count - 1);\r\n                    }}\r\n                    \r\n                    style={{ fontSize: \"25px\" }}\r\n                    >\r\n                    😍\r\n                </ButtonAsLink>\r\n            ) : (\r\n                    <ButtonAsLink\r\n                        onClick={() => {\r\n                            toggleFavorite();\r\n                            setFavorited(true);\r\n                            setCount(count + 1);\r\n                        }}\r\n\r\n                        style={{ fontSize: \"25px\" }}\r\n                    >\r\n                        🙂\r\n                    </ButtonAsLink>\r\n                )}\r\n        : {count}\r\n        </React.Fragment>\r\n    );\r\n};\r\n\r\nexport default FavoriteNote;"},"lineCount":null}},"error":null,"hash":"819078d67c718501affd0cc57733276c","cacheData":{"env":{}}}